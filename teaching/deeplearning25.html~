      <section class="resume-section p-3 p-lg-5 d-flex justify-content-center" id="books">
        <div class="w-100">
          <h2 class="">Reference Materials</h2>
          <br/>

          <h4 class="">Classical Machine Learning <br/></h4>
          
          <div class="resume-item d-flex flex-column flex-md-row justify-content-between">
            <div class="resume-content">
